{
	"manifest_version": 2,
	"name": "To-Do List",
	"description": "Keep track of your tasks easily.",
	"version": "1.0.0",
	"icons": {"128":"icon.png"},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [
  	{
    	"matches": ["*://*/"],
    	"js": ["js/popup.js"]
  	}
	],
	"permissions": ["tabs"]
}
